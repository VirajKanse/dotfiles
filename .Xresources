! ROFI Dracula Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #282a36, #6272a4, #282a36
rofi.color-normal: #282a36, #f7f7f1, #282a36, #F4F99D, #282a36
